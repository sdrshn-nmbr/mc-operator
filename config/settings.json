{
  "execution": {
    "mode": "accuracy",
    "maxIterations": 200,
    "retryAttempts": 3,
    "defaultTimeout": 30000
  },
  "logging": {
    "enabled": true,
    "level": "info",
    "directory": "logs"
  },
  "llm": {
    "model": "claude-3-7-sonnet-latest",
    "temperature": 0.5,
    "maxTokens": 4096
  },
  "templatePaths": {
    "base": "templates",
    "tasks": "templates/tasks"
  },
  "templates": {
    "base_dir": "templates",
    "system_dir": "templates/system",
    "tasks_dir": "templates/tasks",
    "generated_dir": "templates/generated"
  },
  "browser": {
    "headless": false,
    "default_viewport": null,
    "args": [
      "--start-maximized"
    ]
  }
}